{
  "compilerOptions": {
    "target": "esnext",
    "types": ["jest", "@testing-library/jest-dom", "vite/client", "vite-plugin-svgr/client"],
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
  },
  "include": [
    "src",
    "src/setupTests.js",
    "vite-env.d.ts"
  ],
}
