    "prepare": "husky install"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^4.2.4",
    "@snyk/protect": "1.1294.3",
    "@web3-react/core": "8.2.3",
    "@web3-react/metamask": "^8.2.4",
    "@web3-react/network": "^8.2.3",
    "@web3-react/walletconnect": "^8.2.3",
    "axios": "1.7.9",
    "bowser": "2.11.0",
    "ethers": "^6.13.4",
    "include-media": "2.0.0",
    "neon-portal": "2.0.2",
    "npm-run-all": "4.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.2.17",
    "token-list": "https://github.com/neonlabsorg/token-list.git#v5.0.0",
    "web3": "4.16.0"
  },
  "devDependencies": {
    "@craco/craco": "7.1.0",
