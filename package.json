    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "autoprefixer": "10.4.20",
    "dotenv": "16.4.7",
    "eslint-plugin-prettier": "5.2.1",
    "http-proxy-middleware": "3.0.3",
    "husky": ">=9.1.7",
    "lint-staged": "^15.2.11",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "react-scripts": "5.0.1",
    "sass": "1.83.0",
    "style-loader": "4.0.0",
    "typescript": "5.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,html,css}": [
      "yarn lint:fix"
    ]
  }
